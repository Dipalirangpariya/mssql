    "@types/json2csv": "^5.0.3",
    "@types/multer": "^1.4.7",
    "@types/sequelize": "^4.28.13",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "fast-csv": "^4.3.6",
    "joi": "^17.6.0",
    "json2csv": "^5.0.7",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "read-excel-file": "^5.4.2",
